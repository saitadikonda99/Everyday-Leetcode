int averageValue(int* nums, int numsSize){

    int sum = 0;
     int check = 0;
        for( int k=0; k<numsSize; k++ ) {
                if( nums[k] % 2 == 0 ) {
                    if( nums[k] % 3 == 0 ) {
                    sum +=nums[k];
                    check++;
                    }
                     
                }
        }
         
         if( check == 0 ) 
         return NULL;
         else {

         int sum2 = sum/check;
        return sum2;
         }
}
